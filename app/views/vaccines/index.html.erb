<p id="notice"><%= notice %></p>

<h1>Listing Vaccines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Controlnumber</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vaccines.each do |vaccine| %>
      <tr>
        <td><%= vaccine.name %></td>
        <td><%= vaccine.controlNumber %></td>
        <td><%= vaccine.description %></td>
        <td><%= link_to 'Show', vaccine %></td>
        <td><%= link_to 'Edit', edit_vaccine_path(vaccine) %></td>
        <td><%= link_to 'Destroy', vaccine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vaccine', new_vaccine_path %>
